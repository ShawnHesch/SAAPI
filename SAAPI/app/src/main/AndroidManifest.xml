<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="saapi.design.com.myapplication" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewPrescription"
            android:label="@string/title_activity_view_prescription"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="saapi.design.com.myapplication.MainActivity" />
        </activity>
        <activity
            android:name=".viewMessage"
            android:label="@string/title_activity_view_message"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="saapi.design.com.myapplication.MainActivity" />
        </activity>
        <activity
            android:name=".activity_login"
            android:label="@string/title_activity_activity_login" >
        </activity>
    </application>

</manifest>
